<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationsVM" type="com.snakesapplication.app.modules.notifications.data.viewmodel.NotificationsVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearNotifications"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_6pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtActivity"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_138pxh"
                    android:layout_weight="0.91"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{notificationsVM.notificationsModel.txtActivity}"
                    style="@style/txtPoppinssemibold16_1" tools:text="@string/lbl_activity"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowunsplash93bshr"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_36pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageUnsplash93BsHR"
                android:layout_height="@dimen/_52pxh"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_width="@dimen/_52pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_thumbnail}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_thumbnail"/>
            <TextView android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.82" android:layout_width="0dp"
                android:text="@{notificationsVM.notificationsModel.txtDescription}"
                style="@style/txtPoppinsregular14" tools:text="@string/msg_bezaleel_nwabia"/>
        </LinearLayout>
        <View android:background="@color/black_900_63"
            android:id="@+id/lineLineSeven"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_335pxh"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowunsplash93bshrOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageUnsplash93BsHROne"
                android:layout_height="@dimen/_52pxh"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_width="@dimen/_52pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_thumbnail}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_thumbnail"/>
            <TextView android:id="@+id/txtDescriptionOne"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.82" android:layout_width="0dp"
                android:text="@{notificationsVM.notificationsModel.txtDescriptionOne}"
                style="@style/txtPoppinsregular14" tools:text="@string/msg_bezaleel_nwabia"/>
        </LinearLayout>
        <View android:background="@color/black_900_63"
            android:id="@+id/lineLineSevenOne"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_335pxh"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowunsplash93bshrTwo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageUnsplash93BsHRTwo"
                android:layout_height="@dimen/_52pxh"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_width="@dimen/_52pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_thumbnail}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_thumbnail"/>
            <TextView android:id="@+id/txtDescriptionTwo"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.82" android:layout_width="0dp"
                android:text="@{notificationsVM.notificationsModel.txtDescriptionTwo}"
                style="@style/txtPoppinsregular14" tools:text="@string/msg_bezaleel_nwabia"/>
        </LinearLayout>
        <View android:background="@color/black_900_63"
            android:id="@+id/lineLineSevenTwo"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_335pxh"/>
    </LinearLayout>
</layout>
